//   Linker control file for the PIC 10F204 processor.
//
CODEPAGE NAME=code0  START=0x0000  END=0x00FF //code page 0
CODEPAGE NAME=idlocs START=0x0100  END=0X0103 PROTECTED //ID words
CODEPAGE NAME=config START=0x0FFF  END=0x0FFF PROTECTED //processor config word

DATABANK NAME=bank0  START=0x10    END=0x1F   //register bank 0

SECTION NAME=.CONFIG ROM=config
