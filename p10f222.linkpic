//   Linker control file for the PIC 10F222 processor.
//
CODEPAGE NAME=code0  START=0x0000  END=0x01FF //code page 0
CODEPAGE NAME=idlocs START=0x0200  END=0X0203 PROTECTED //ID words
CODEPAGE NAME=config START=0x0FFF  END=0x0FFF PROTECTED //processor config word

DATABANK NAME=bank0  START=0x09    END=0x1F   //register bank 0

SECTION NAME=.CONFIG ROM=config
